{
  "name": "indent-detective",
  "main": "./lib/indent-detective",
  "version": "0.4.0",
  "description": "Detect indentation settings in the active editor",
  "keywords": [],
  "repository": "https://github.com/JunoLab/atom-indent-detective",
  "license": "MIT",
  "scripts": {
    "typescript": "tsc -p ./lib_src/tsconfig.json",
    "prettier_src": "prettier --write ./lib_src/**/*.ts",
    "prettier_out": "prettier --write ./lib/**/*.js",
    "babel": "babel lib --out-dir lib",
    "lint": "eslint . --ext js,ts,json",
    "build": "npm run typescript -s & npm run babel -s && npm run prettier_out -s"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-select-list": "^0.7.2"
  },
  "devDependencies": {
    "@types/atom": "^1.40.1",
    "typescript": "^3.8.3",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-only-warn": "latest",
    "prettier": "latest",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "babel-preset-minify": "^0.5.1",
    "@babel/plugin-transform-for-of": "^7.8.6",
    "@babel/core": "^7.8.7",
    "@babel/cli": "^7.8.4"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "prettier": {
    "semi": false,
    "printWidth": 110,
    "tabWidth": 4
  },
  "babel": {
    "presets": [
      [
        "minify",
        {
          "deadcode": false,
          "mangle": false,
          "booleans": false,
          "builtIns": false,
          "flipComparisons": false,
          "infinity": false,
          "memberExpressions": false,
          "simplifyComparisons": false,
          "undefinedToVoid": false
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-for-of",
        {
          "assumeArray": true
        }
      ]
    ]
  }
}
